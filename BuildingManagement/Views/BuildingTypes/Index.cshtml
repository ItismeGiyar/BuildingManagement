            <td>
                @Html.DisplayFor(modelItem => item.BdtypDesc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CmpyId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RevDteTime)
            </td>
            <td>
