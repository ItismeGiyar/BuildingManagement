@model BuildingManagement.Models.Autonumber




@{
    ViewData["Title"] = "Auto Number";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-6">
                <h4>@ViewData["Title"]</h4>
            </div>
            <div class="col-6">
                <div class="breadcrumb float-right active">
                    <a asp-action="Index" class="btn btn-primary">Back to List <i class="fas fa-back"></i></a>

                </div>
            </div>
        </div>
    </div>
</section>
<section class="content">
    <div class="container-fluid">
        <div class="card card-gray">
            <div class="card-header">
                <h3 class="card-title">
                    Are you sure you want to delete this?
                </h3>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-12">

                        <dl class="row">
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.AutoNoId)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.AutoNoId)
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.BillPrefix)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.BillPrefix)
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.BizDte)
                            </dt>
                            <dd class="col-sm-10">
                                @Convert.ToDateTime(Model.BizDte).ToString("dd-MM-yyyy")
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.ZeroLeading)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.ZeroLeading)
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.RunningNo)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.RunningNo)
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.LastUsedNo)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.LastUsedNo)
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.LastGenerateDte)
                            </dt>
                            <dd class="col-sm-10">
                                @Convert.ToDateTime(Model.LastGenerateDte).ToString("dd-MM-yyyy")
                            </dd>
                           



                        </dl>
                    </div>
                </div>
            </div>
            <div class="card-footer">
                <form asp-action="Delete">
                    <input type="hidden" asp-for="AutoNoId" />
                    <input type="submit" value="Delete" class="btn btn-danger" />
                </form>

            </div>
        </div>
    </div>
</section>



