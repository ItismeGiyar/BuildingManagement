@model BuildingManagement.Models.Menuaccess



@{
    ViewData["Title"] = "Menu Access";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-6">
                <h4>@ViewData["Title"]</h4>
            </div>
            <div class="col-6">
                <div class="breadcrumb float-right active">
                    <a asp-action="Index" class="btn btn-primary">Back to List <i class="fas fa-back"></i></a>

                </div>
            </div>
        </div>
    </div>
</section>
<section class="content">
    <div class="container-fluid">
        <div class="card card-gray">
            <div class="card-header">
                <h3 class="card-title">@ViewData["Title"] Details</h3>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-12">
    <dl class="row">
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.AccessId)
        </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.AccessId)
        </dd>
                            <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.BtnNme)
        </dt>
                            <dd class="col-sm-10">
            @Html.DisplayFor(model => model.BtnNme)
        </dd>
                            <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RevDteTime)
        </dt>
                            <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RevDteTime)
        </dd>
    </dl>

                    </div>
                </div>
            </div>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.AccessId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
</section>


